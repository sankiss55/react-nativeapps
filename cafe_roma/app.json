{
  "expo": {
    "name": "Cafe Roma Negocios",
    "slug": "cafe_roma",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./img/logo_app.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./img/logo_app.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./img/logo_app.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.santiago_vs.cafe_roma",
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "favicon": "./img/logo_app.png"
    },
    "plugins": [
      "expo-font",
      "expo-barcode-scanner",
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "568b62be-fc4b-4b5c-9926-db3cf0fca14f"
      }
    },
    "owner": "sanprogramer1"
  }
}
